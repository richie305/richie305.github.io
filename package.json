{
  "name": "cursor_paws",
  "version": "1.0.0",
  "description": "A modern web application to track your dog's food and bathroom behavior, with persistent favorites and cloud storage. Built with HTML, CSS, JavaScript, and [Supabase](https://supabase.com/) as the backend database.",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npx serve",
    "test": "jest --config jest.config.cjs --testTimeout=30000",
    "test:watch": "jest --watch --config jest.config.cjs --testTimeout=30000",
    "test:coverage": "jest --coverage --config jest.config.cjs --testTimeout=30000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:install": "cypress install",
    "build": "mkdir -p dist && cp -r index.html styles.css app.js dist/ && cp -r molly.png dist/",
    "deploy": "npm run build && touch dist/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richie305/richie305.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/richie305/richie305.github.io/issues"
  },
  "homepage": "https://github.com/richie305/richie305.github.io#readme",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "babel-jest": "^29.7.0",
    "cypress": "^13.17.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "serve": "^14.2.4"
  },
  "dependencies": {
    "lint": "^0.8.19"
  }
}
